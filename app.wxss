/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.text-center {
  text-align: center;
}

/* 头部左边部分图片样式 */

.header-left-wrap {
  width: 70rpx;
  height: 70rpx;
  position: relative;
  left: -30rpx;
}

.header-left-wrap image.image-btn {
  width: 100%;
  height: 100%;
}
